{
  "name": "@tadashi/hash",
  "version": "2.0.0",
  "description": "Create a hash from any value",
  "keywords": [
    "key",
    "value",
    "hash",
    "lib"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/hash.git"
  },
  "homepage": "https://github.com/lagden/hash#readme",
  "bugs": {
    "url": "https://github.com/lagden/hash/issues"
  },
  "main": "src/hash.js",
  "type": "module",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "pretest": "xo",
    "test": "c8 --reporter=text --reporter=lcov ava"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "c8": "7.7.2",
    "xo": "0.39.1"
  },
  "xo": {
    "ignores": [
      "bin/**/*.js"
    ],
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "unicorn/prevent-abbreviations": 0,
      "promise/prefer-await-to-then": 0
    }
  }
}
